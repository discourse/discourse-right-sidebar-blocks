<PluginOutlet @name="above-right-sidebar-blocks" />

{{#each blocks as |block|}}
  <div class={{block.classNames}}>
    {{component block.name params=block.parsedParams}}
  </div>
  <PluginOutlet
    @name="below-right-sidebar-block"
    @outletArgs={{hash block=block}}
  />
{{/each}}

